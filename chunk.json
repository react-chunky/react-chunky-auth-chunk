{
  "name": "auth",
  "routes": {
    "loading": {
      "retrieveDataOnMount": true,
      "actions": ["cache://retrieve/auth"]
    },
    "login": {
      "actions": ["firebase://login/email"],
      "dark": true
    }
  },
  "transitions": ["replace://login#showLogin", "replace://#showDashboard"]
}
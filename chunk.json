{
  "name": "auth",
  "routes": {
    "loading": {
      "operations": {
        "checkUser": "cache://retrieve/account", 
        "cleanUp": "local://delete"
      }
    },
    "login": {
      "startOperationsOnMount": false,
      "operations": {
        "signIn": "firebase://login/email?email&password", 
        "signUp": "firebase://register/email?email&password"
      },
      "dark": false
    }
  },
  "transitions": ["replace://login", "replace://dashboard"]
}